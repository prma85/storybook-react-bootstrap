  "license": "MIT",
  "bugs": {
    "url": "https://github.com/prma85/storybook-react-bootstrap/issues"
  },
  "homepage": "https://github.com/prma85/storybook-react-bootstrap#readme",
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-env": "^7.12.1",
    "@babel/preset-react": "^7.12.1",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-a11y": "^6.0.27",
    "@storybook/addon-actions": "^6.0.27",
    "@storybook/addon-backgrounds": "^6.0.27",
    "@storybook/addon-console": "^1.2.2",
    "@storybook/addon-links": "^6.0.27",
    "@storybook/addon-storysource": "^6.0.27",
    "@storybook/addon-viewport": "^6.0.27",
    "@storybook/addons": "^6.0.27",
    "@storybook/node-logger": "^6.0.27",
    "@storybook/preset-create-react-app": "^3.1.4",
    "@storybook/react": "^6.0.27",
    "@storybook/source-loader": "^6.0.27",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.6.0",
    "@typescript-eslint/parser": "^4.6.0",
    "babel-loader": "^8.1.0",
    "core-js": "^3.6.5",
    "eslint": "^7.12.0",
    "eslint-plugin-react": "^7.21.5",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-is": "^16.13.1",
