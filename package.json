{
  "name": "storybook-react-bootstrap",
  "version": "1.0.0",
  "description": "This is an example of creating a storybook website stand alone with react-bootstrap or ant design or reakit and typescript",
  "main": "index.js",
  "scripts": {
    "prepublishOnly": "yarn clean && yarn build",
    "test": "exit 0",
    "test:smoke": "yarn start -- --smoke-test --ci",
    "clean": "rimraf dist",
    "start": "start-storybook -p 3001",
    "build": "build-storybook -o dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/prma85/storybook-react-bootstrap.git"
  },
  "keywords": [
